<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="model.TaskHist" table="Task_hist" catalog="CIS">
        <id name="Oid" type="java.lang.Integer">
            <column name="Oid" />
            <generator class="identity" />
        </id>        
        
        <property name="Task_apply_oid" type="java.lang.Integer">
            <column name="Task_apply_oid" />
        </property>
        
        <property name="empl" type="java.lang.String">
            <column name="empl" />
        </property>
        
        <property name="reply" type="java.lang.String">
            <column name="reply" />
        </property>
        
        <property name="edate" type="java.util.Date">
            <column name="edate" />
        </property>
        
        
        
        
        
    </class>
</hibernate-mapping>