<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="model.TaskApply" table="Task_apply" catalog="CIS">
        <id name="Oid" type="java.lang.Integer">
            <column name="Oid" />
            <generator class="identity" />
        </id>
        <property name="Task" type="java.lang.Integer">
            <column name="Task" />
        </property>
        
        <property name="from_empl" type="java.lang.String">
            <column name="from_empl" />
        </property>
        
        <property name="from_stmd" type="java.lang.String">
            <column name="from_stmd" />
        </property>
        
        <property name="note" type="java.lang.String">
            <column name="note" />
        </property>
        
        <property name="next_empl" type="java.lang.Integer">
            <column name="next_empl" />
        </property>
        
        <property name="edate" type="java.util.Date">
            <column name="edate" />
        </property>
        
        <property name="status" type="java.lang.String">
            <column name="status" />
        </property>
        
        <property name="email" type="java.lang.String">
            <column name="email" />
        </property>
        
        <property name="sdate" type="java.util.Date">
            <column name="sdate" />
        </property>
        
    </class>
</hibernate-mapping>